Функция renderLinksNoBlock(product) Экспорт
	
	href = "";
	
	Если product.view.Свойство("wildberriesId")   
		И product.Свойство("wildberriesSizeId")
		И ЗначениеЗаполнено(product.view.wildberriesId)
	    И ЗначениеЗаполнено(product.wildberriesSizeId) Тогда		
		href = "https://www.wildberries.ru/catalog/" + product.view.wildberriesId + "/detail.aspx?size=" + product.wildberriesSizeId;
	КонецЕсли;
	
	
	Если product.Свойство("ozonFbsSku")
	    И ЗначениеЗаполнено(product.ozonFbsSku) Тогда		
		href= "https://www.ozon.ru/product/" + product.ozonFbsSku;
	КонецЕсли;	

	Если product.Свойство("ozonId")
	    И ЗначениеЗаполнено(product.ozonId) Тогда		
		href= "https://www.ozon.ru/product/" + product.ozonId;
	КонецЕсли;	
	
	Если product.Свойство("yandexMarketSku")
	    И ЗначениеЗаполнено(product.yandexMarketSku) Тогда		
		href = "https://pokupki.market.yandex.ru/product/" + product.yandexMarketSku;
	КонецЕсли;    
	
	Если product.view.model.Свойство("aliexpressId")
	    И ЗначениеЗаполнено(product.yandexMarketSku) Тогда		
		href = "https://aliexpress.ru/item/" + product.view.model.aliexpressId + ".html";
	КонецЕсли;

	Если product.Свойство("moySkladLinkId")
	    И ЗначениеЗаполнено(product.moySkladLinkId) Тогда		
		href = "https://online.moysklad.ru/app/#good/edit?id=" + product.moySkladLinkId;
	КонецЕсли;

	Если product.Свойство("sberMegaMarketId")
	    И ЗначениеЗаполнено(product.sberMegaMarketId) Тогда		
		href = "https://sbermegamarket.ru/catalog/?q=" + product.sberMegaMarketId;
	КонецЕсли;
	
	Если product.Свойство("avitoId")
	    И ЗначениеЗаполнено(product.avitoId) Тогда		
		href = "https://www.avito.ru/" + product.avitoId;
	КонецЕсли;
	
	Возврат СтрЗаменить(href, " ", "");

