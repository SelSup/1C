	( 
	"if (WScript.Arguments.Count()==0) 
	| WScript.Quit(); 
	|else 
	| if (isNaN(parseInt(WScript.Arguments(0)))) 
	| WScript.Quit(); 
	|WScript.Sleep(WScript.Arguments(0));" 
	); 
	Скрипт.Записать(КаталогВременныхФайлов()+"sleep.js", КодировкаТекста.ANSI); 
