	ТаблицаТоваровSelSup = Новый ТаблицаЗначений(); 
	ТаблицаТоваровSelSup.Колонки.Добавить("Склад", Новый ОписаниеТипов("СправочникСсылка.Склады")); 
	ТаблицаТоваровSelSup.Колонки.Добавить("oneCId", Новый ОписаниеТипов("УникальныйИдентификатор"));   
	ТаблицаТоваровSelSup.Колонки.Добавить("ОстатокSelSup", Новый ОписаниеТипов("Число"));
	ТаблицаТоваровSelSup.Колонки.Добавить("ОжидаетсяКПоставкеSelSup", Новый ОписаниеТипов("Число"));
	ТаблицаТоваровSelSup.Колонки.Добавить("ВРезервеSelSup", Новый ОписаниеТипов("Число"));  
	
	возврат ТаблицаТоваровSelSup;
	
	// Находим список товаров, имеющихся в ЛК
	Страница = 0;
	
	Пока Истина Цикл
		
		Страница = Страница + 1;
		
		РезультатЗапроса = ИнформацияОСтокахТоваров(Страница, СкладыONEC);  
		
		Если РезультатЗапроса = Неопределено Тогда
			Возврат ТаблицаТоваровSelSup;
		КонецЕсли;
		
		Попытка
			МассивТоваров = РезультатЗапроса["rows"];
			Для каждого СтрокаТовара Из МассивТоваров Цикл
				
				Если Не СтрокаТовара.Свойство("oneCId") Тогда
					Продолжить;
				КонецЕсли; 
				
				oneCId = СтрокаТовара.oneCId;
				Если НЕ (СтрНайти(oneCId, "R") = 0 
					И Не СтрНайти(oneCId, "-") = 0   //TODO  
					И СтрНайти(oneCId, "test") = 0) Тогда  //TODO
					Продолжить;; 
				КонецЕсли;				
				
				НоваяСтрока = ТаблицаТоваровSelSup.Добавить(); 
				
				//Отбор = Новый Структура();
				//Отбор.Вставить("warehouseId", СтрокаТовара.warehouseId); //TODO
				//Строки = Склады.НайтиСтроки(Отбор);
				//Если Строки.Количество() > 0 Тогда
				//	НоваяСтрока.Склад = Строки[0].Склад;
				//КонецЕсли;  
				
				НоваяСтрока.oneCId = Новый УникальныйИдентификатор(oneCId); 
				НоваяСтрока.ОстатокSelSup               = СтрокаТовара["instockQuantity"];
				//НоваяСтрока.ОжидаетсяКПоставкеSelSup  = СтрокаТовара["coming"];
				НоваяСтрока.ВРезервеSelSup 	            = ?(СтрокаТовара.Свойство("reserved"), СтрокаТовара["reserved"], 0);						   
				
			КонецЦикла;
			Если МассивТоваров.Количество() < 200 Тогда
				Прервать;
			КонецЕсли;
		Исключение
			СообщитьОбОшибке("Ошибка при чтении ответа от сервера: " + ОписаниеОшибки());
			Возврат ТаблицаТоваровSelSup;
		КонецПопытки;
		
	КонецЦикла; 
	
	Возврат ТаблицаТоваровSelSup;
	
