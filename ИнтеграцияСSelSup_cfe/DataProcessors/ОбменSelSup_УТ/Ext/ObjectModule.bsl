				СообщитьОбОшибке("Ошибка при чтении ответа от сервера: " + ОписаниеОшибки());
				Возврат;
			КонецПопытки;
			
		КонецЦикла;
	КонецЕсли; 
		
	// Заполняем остатки
	Для каждого СтрокаТовара Из Товары Цикл
		
		ЗаполнитьНоменклатуру(СтрокаТовара);
