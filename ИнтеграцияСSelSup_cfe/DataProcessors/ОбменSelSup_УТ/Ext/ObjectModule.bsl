Функция НайтиКлючAPI(ТекОрганизация) 
	
	Если Метаданные.РегистрыСведений.Найти("SS_КлючиAPI") = Неопределено Тогда 
		Возврат Неопределено; 
	КонецЕсли;	
	
	Запрос = Новый Запрос;
	ТекстЗапроса = "ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	SS_КлючиAPI.КлючAPI КАК КлючAPI,
	|	SS_КлючиAPI.Организация КАК Организация
	|ИЗ
	|	РегистрСведений.SS_КлючиAPI КАК SS_КлючиAPI
	|ГДЕ
	|	SS_КлючиAPI.Организация = &Организация
	|	И SS_КлючиAPI.Использовать";	
	
	Запрос.Текст = ТекстЗапроса; 
	
	Запрос.УстановитьПараметр("Организация", ТекОрганизация);
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда
		Возврат Выборка;
	Иначе         
		ТекстСообщения = "Не найден ключ API по организации: " + ТекОрганизация + " в настройках интеграции SelSup."; 
